  <DataSets>
    <DataSet Name="dstMagazaSatis">
      <Fields>
        <Field Name="MagazaAdi">
          <DataField>MagazaAdi</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SatısToplamı">
          <DataField>SatısToplamı</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>dsYW</DataSourceName>
        <CommandText>SELECT     TOP (10) Magaza.MagazaAdi, SUM(FaturaSatirlari.Tutar) AS SatısToplamı
FROM         Magaza INNER JOIN
                      Fatura ON Magaza.MagazaId = Fatura.MagazaId INNER JOIN
                      FaturaSatirlari ON Fatura.FaturaId = FaturaSatirlari.FaturaId
GROUP BY Magaza.MagazaAdi
ORDER BY SatısToplamı DESC</CommandText>
      </Query>
    </DataSet>
  </DataSets>
